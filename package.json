{
  "name": "ai-chatbot-backend",
  "version": "1.0.0",
  "description": "AI Chatbot Backend converted from Python to Node.js with Express and TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "keywords": ["ai", "chatbot", "express", "typescript", "gemini"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.15.0",
    "@google-cloud/text-to-speech": "^5.2.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "wikipedia": "^2.1.2",
    "axios": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.14",
    "@types/multer": "^1.4.7",
    "@types/compression": "^1.7.3",
    "@types/morgan": "^1.9.5",
    "typescript": "^5.2.2",
    "ts-node-dev": "^2.0.0",
    "rimraf": "^5.0.5"
  }
}